FoamFile
{
    version     2.0;
    format      binary;
    class       surfaceScalarField;
    location    "0.002";
    object      alphaQGD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.5;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    axes
    {
        type            fixedValue;
        value           nonuniform 
0
;
    }
    walls
    {
        type            calculated;
        value           uniform 0;
    }
    wedge1
    {
        type            wedge;
        value           uniform 0;
    }
    wedge2
    {
        type            wedge;
        value           uniform 0;
    }
}


